    "babel-preset-minify": "^0.5.1",
    "cpy-cli": "^3.0.0",
    "del-cli": "^3.0.0",
    "http-server": "^0.12.1",
    "rollup": "^1.29.1",
    "rollup-plugin-babel": "^4.3.3",
